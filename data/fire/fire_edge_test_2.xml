<!DOCTYPE xml>
<xml>
  <General>
    <Name>Fire Simulation</Name>
    <Type>Fire</Type>
    <Author>Marc Chmielewski</Author>
    <Description>Edge test that includes some already-burnt trees</Description>
  </General>
  <GeometricConfiguration>
    <CellShape>Rectangle</CellShape>
    <Height>10</Height>
    <Width>10</Width>
  </GeometricConfiguration>
  <SimulationParameters>
    <Flammability>.5</Flammability>
  </SimulationParameters>
  <!--
  Initial states should only be specified for non-default states, i.e. only specify
  initially live cells.
   -->
  <InitialStates>
    <Cell>
      <Row>0</Row>
      <Column>0</Column>
      <State>1</State>
    </Cell>
    <Cell>
      <Row>0</Row>
      <Column>4</Column>
      <State>1</State>
    </Cell>
    <Cell>
      <Row>0</Row>
      <Column>9</Column>
      <State>1</State>
    </Cell>
    <Cell>
      <Row>1</Row>
      <Column>2</Column>
      <State>2</State>
    </Cell>
    <Cell>
      <Row>1</Row>
      <Column>5</Column>
      <State>2</State>
    </Cell>
  </InitialStates>
</xml>
