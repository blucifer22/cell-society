<!DOCTYPE xml>
<xml>
  <General>
    <Name>Foraging Ants: Test 1</Name>
    <Type>Ant</Type>
    <Author>Marc Chmielewski</Author>
    <Description>A test for foraging ants</Description>
  </General>
  <GeometricConfiguration>
    <CellShape>Rectangle</CellShape>
    <Height>5</Height>
    <Width>5</Width>
  </GeometricConfiguration>
  <SimulationParameters>
    <PheromoneEvaporationRate>1</PheromoneEvaporationRate>
    <TargetPheromoneConcentration>100</TargetPheromoneConcentration>
  </SimulationParameters>
  <!--
  Initial states should only be specified for non-default states, i.e. only specify
  initially live cells.
   -->
  <InitialStates>
    <Cell>
      <Row>0</Row>
      <Column>2</Column>
      <State>3</State>
    </Cell>
    <Cell>
      <Row>4</Row>
      <Column>2</Column>
      <State>2</State>
    </Cell>
    <Cell>
      <Row>1</Row>
      <Column>2</Column>
      <State>1</State>
    </Cell>
  </InitialStates>
</xml>