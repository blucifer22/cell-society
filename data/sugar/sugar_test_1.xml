<!DOCTYPE xml>
<xml>
  <General>
    <Name>SugarScape: Test 1</Name>
    <Type>Sugar</Type>
    <Author>Marc Chmielewski</Author>
    <Description>A test for SugarScape</Description>
  </General>
  <GeometricConfiguration>
    <CellShape>Rectangle</CellShape>
    <Height>5</Height>
    <Width>5</Width>
  </GeometricConfiguration>
  <SimulationParameters>
    <MaxSugarCapacity>25</MaxSugarCapacity>
    <SugarRegrowthRate>1</SugarRegrowthRate>
    <SugarMetabolismRate>5</SugarMetabolismRate>
  </SimulationParameters>
  <!--
  Initial states should only be specified for non-default states, i.e. only specify
  initially live cells.
   -->
  <InitialStates>
    <Cell>
      <Row>0</Row>
      <Column>0</Column>
      <State>1</State>
    </Cell>
    <Cell>
      <Row>2</Row>
      <Column>1</Column>
      <State>1</State>
    </Cell>
    <Cell>
      <Row>3</Row>
      <Column>2</Column>
      <State>1</State>
    </Cell>
  </InitialStates>
</xml>